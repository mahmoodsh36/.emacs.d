\begin{algorithm}[H]
  \DontPrintSemicolon
  \TitleOfAlgo{${1:title}}
  \KwIn{${2:input}}
  \KwOut{${3:output}}
  \BlankLine
  \SetKwFunction{FMain}{\textsc{${4:FunctionName}}}
  \SetKwProg{Fn}{}{:}{}
  \Fn{\FMain{${5:arguments}}} {
    $0
  }
\end{algorithm}