\begin{algorithm}[H]
  \DontPrintSemicolon
  \TitleOfAlgo{${1:title}}
  \KwIn{${2:input}}
  \KwOut{${3:output}}
  \BlankLine
  \SetKwFunction{func}{\textsc{${4:FunctionName}}}
  \SetKwProg{prog}{}{:}{}
  \prog{\func{${5:arguments}}} {
    $0
  }
\end{algorithm}