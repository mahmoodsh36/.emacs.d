        "rules": [
          {
            "description": "keybinding to open a terminal",
            "manipulators": [
              {
                "from": {
                  "key_code": "return_or_enter",
                  "modifiers": {
                    "mandatory": [
                      "right_command"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a kitty"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "keybinding to open chrome",
            "manipulators": [
